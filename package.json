{
  "name": "cissp-tester",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && touch out/.nojekyll",
    "deploy": "npm run export && gh-pages -d out -t true",
    "test-prod": "npm run build && npx serve out -l 8080",
    "clean": "rm -rf .next out",
    "generate-cache": "npx tsx src/scripts/generate-question-cache.ts",
    "analyze-domains": "npx tsx src/scripts/analyze-domains.ts",
    "tag-questions": "npm run generate-cache && npm run analyze-domains"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "next": "^14.2.31",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/html2canvas": "^1.0.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.33.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
